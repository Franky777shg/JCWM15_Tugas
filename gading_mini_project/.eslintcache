     {item.description}\r\n              </Card.Text>\r\n              <Button variant=\"danger\" href={item.url}>\r\n                Read more\r\n              </Button>\r\n            </Card.Body>\r\n          </Card>\r\n        </Col>\r\n      );\r\n    });\r\n  };\r\n  //#endregion\r\n\r\n  //#region List Menu\r\n  listCategoryMenu = () => {\r\n      return this.state.showCategory.map((category) => (\r\n        <Dropdown.Item\r\n            onClick={() => this.handleChangeCategory(category)}\r\n            key={category}\r\n            href=\"#/action-1\">\r\n            {category}\r\n        </Dropdown.Item>\r\n        ))\r\n  }\r\n\r\n  listCountryMenu = () => {\r\n      return this.state.showCountry.map((country) => (\r\n        <Dropdown.Item\r\n            onClick={() => this.handleChangeCountry(country)}\r\n            key={country}\r\n            href=\"#/action-1\">\r\n            {country}\r\n        </Dropdown.Item>\r\n        ))\r\n  }\r\n  //#endregion\r\n\r\n  //#region LIFECYCLE\r\n  componentDidMount() {\r\n    this.fetchDataNews(this.state.category, this.state.country);\r\n  }\r\n  //#endregion\r\n  \r\n  render() {\r\n    return (\r\n      <div>\r\n        <Container fluid>\r\n          <Row style={{ padding: \"0% 15%\" }}>\r\n            <Col md={12} style={{ textAlign: \"center\" }}>\r\n              <p style={{ fontSize: \"100px\", fontWeight: \"bolder\" }}>\r\n                Dunia Dalam Berita.\r\n              </p>\r\n            </Col>\r\n          </Row>\r\n          <Row style={{ padding: \"0% 15%\" }}>\r\n            <Col md={12} style={{ textAlign: \"center\" }}>\r\n              <p style={{ fontSize: \"80px\", fontWeight: \"bolder\", opacity: \"80%\",}}>\r\n                Dunia Dalam Berita.\r\n              </p>\r\n            </Col>\r\n          </Row>\r\n          <Row style={{ padding: \"0% 15%\" }}>\r\n            <Col md={12} style={{ textAlign: \"center\" }}>\r\n              <p style={{ fontSize: \"60px\", fontWeight: \"bold